{
  "name": "prash/php",
  "description": "A simple pre-setup for php in a linux distro.",
  "autoload": {
    "psr-4": {
      "App\\": "app/",
     "Database\\": "Database/"
    },
    "files": [
      "app/Helper.php"
    ]
  },
  "authors": [
    {
      "name": "prash"
    }
  ],
  "require": {
    "php": ">=8.0",
    "vlucas/phpdotenv": "^5.6"
  },
  "scripts": {
    "serve": "php -S 127.0.0.1:8000 -t public/",
    "dev": "php -S 127.0.0.1:8000 -t public/"
  }
}
